{
    "name": "nayjest/di-hub",
    "authors": [
        {
            "name": "v.stepanenko",
            "email": "mail@vitaliy.in"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nayjest\\DI\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nayjest\\DI\\Test\\" : "tests/unit",
            "Nayjest\\DI\\Example\\" : "tests/examples"
        }
    },
    "require": {
        "symfony/var-dumper": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "test": "php vendor/phpunit/phpunit/phpunit",
        "unit-test": "php vendor/phpunit/phpunit/phpunit --testsuite unit"
    }
}
